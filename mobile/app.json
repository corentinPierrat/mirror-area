{
  "expo": {
    "name": "mobile",
    "scheme": "mobileapp",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Trigger.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.theotime6272.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization",
      "expo-web-browser",
        "expo-font",
        [
          "expo-image-picker",
          {
            "photosPermission": "L'application a besoin d'accéder à vos photos pour mettre à jour votre image de profil."
          }
        ]
    ],
    "extra": {
      "eas": {
        "projectId": "c740dcdd-09f0-4a33-a0d4-4295edc4bec4"
      }
    }
  }
}
